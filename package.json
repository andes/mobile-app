{
    "name": "mobile_app",
    "version": "5.2.1",
    "author": "Andes NeuquÃ©n",
    "description": "Andes mobile",
    "homepage": "https://github.com/andes/mobile-app",
    "publishConfig": {},
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "version:update": "npm version \"$(ionic config get version | tr -d \\')\" --allow-same-version && config-cordova -config=config.xml set version \"$(ionic config get version | tr -d \\')\" && echo Version: $(ionic config get version)",
        "config:demo": "config-cordova -config=config.xml set id \"org.andesDev.mobile\" && cp google-services-demo.json platforms/android/app/google-services.json",
        "prepare:demo": "cordova-res --icon-source resources/demo/icon.png --splash-source resources/demo/splash.png --icon-foreground-source resources/demo/icon.png --icon-background-source resources/demo/icon.png",
        "compile:demo:android": "ionic cordova build android --release",
        "compile:demo:ios": "ionic cordova build ios --release",
        "bundle:demo:android": "ionic cordova build android --release -- -- --packageType=bundle",
        "sign:demo:apk": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore andes-key.jks -storepass $(cat build-andes.json | json android.release.storePassword) -keypass $(cat build-andes.json | json android.release.password) $PWD/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk andes-release",
        "zipalign:demo:apk": "${ANDROID_SDK_ROOT:-$ANDROID_HOME}/build-tools/$(ionic config get buildToolsVersion | tr -d \\')/zipalign -v 4 $PWD/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk \"andesDemo-v$(ionic config get version | tr -d \\').apk\"",
        "zipalign:demo:aab": "${ANDROID_SDK_ROOT:-$ANDROID_HOME}/build-tools/$(ionic config get buildToolsVersion | tr -d \\')/zipalign -v 4 $PWD/platforms/android/app/build/outputs/bundle/release/app-release.aab \"andesDemo-v$(ionic config get version | tr -d \\').aab\"",
        "sign:demo:aab": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore andes-key.jks -storepass $(cat build-andes.json | json android.release.storePassword) -keypass $(cat build-andes.json | json android.release.password) $PWD/platforms/android/app/build/outputs/bundle/release/app-release.aab andes-release",
        "build:demo:android:aab": "npm run config:demo && npm run prepare:demo && npm run bundle:demo:android && npm run sign:demo:aab && npm run zipalign:demo:aab",
        "build:demo:android:apk": "npm run config:demo && npm run prepare:demo && npm run compile:demo:android && npm run sign:demo:apk && npm run zipalign:demo:apk",
        "build:demo:ios": "npm run config:demo && npm run prepare:demo && npm run compile:demo:ios",
        "config:prod": "config-cordova -config=config.xml set id \"org.andesDev.mobile\" && cp google-services-prod.json platforms/android/app/google-services.json",
        "prepare:prod": "cordova-res",
        "config:ios": "config-cordova -config=config.xml set id \"ar.gob.andesDev.mobile\"",
        "prepare:demo:ios": "ionic cordova prepare ios",
        "config:prod:ios": "ionic cordova prepare ios --configuration production",
        "compile:prod:android": "cp google-services-prod.json platforms/android/app/google-services.json && ionic cordova build android --configuration production --release",
        "compile:prod:ios": "ionic cordova build ios --configuration production",
        "bundle:prod:android": "ionic cordova build android --configuration production --release -- -- --packageType=bundle",
        "sign:prod:apk": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore andes-key.jks -storepass $(cat build-andes.json | json android.release.storePassword) -keypass $(cat build-andes.json | json android.release.password) $PWD/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk andes-release",
        "sign:prod:aab": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore andes-key.jks -storepass $(cat build-andes.json | json android.release.storePassword) -keypass $(cat build-andes.json | json android.release.password) $PWD/platforms/android/app/build/outputs/bundle/release/app-release.aab andes-release",
        "zipalign:prod:apk": "${ANDROID_SDK_ROOT:-$ANDROID_HOME}/build-tools/$(ionic config get buildToolsVersion | tr -d \\')/zipalign -v 4 $PWD/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk \"andesProd-v$(ionic config get version | tr -d \\').apk\"",
        "zipalign:prod:aab": "${ANDROID_SDK_ROOT:-$ANDROID_HOME}/build-tools/$(ionic config get buildToolsVersion | tr -d \\')/zipalign -v 4 $PWD/platforms/android/app/build/outputs/bundle/release/app-release.aab \"andesProd-v$(ionic config get version | tr -d \\').aab\"",
        "build:prod:android:apk": "npm run config:prod && npm run prepare:prod && npm run compile:prod:android && npm run sign:prod:apk && npm run zipalign:prod:apk",
        "build:prod:android:aab": "npm run prepare:prod && npm run bundle:prod:android && npm run sign:prod:aab && npm run zipalign:prod:aab",
        "build:prod:ios": "npm run config:prod && npm run compile:prod:ios",
        "build:demo:apks": "java -jar resources/android/tools/bundletool-all-1.8.2.jar build-apks --mode=universal --bundle=$npm_config_aab --output=$npm_config_apks --ks=andes-key.jks --ks-pass=pass:$(cat build-andes.json | json android.debug.storePassword) --ks-key-alias=andes-debug --key-pass=pass:$(cat build-andes.json | json android.debug.password)",
        "build:demo:apk": "unzip -p $npm_config_apks universal.apk > $npm_config_apk",
        "test": "ng test",
        "lint": "eslint -c .eslintrc.js --ext .ts .",
        "lint:fix": "eslint -c .eslintrc.js --ext .ts . --fix",
        "e2e": "ng e2e",
        "build:browser": "ionic cordova build browser --configuration production --no-interactive --confirm",
        "android:reinstall": "ionic cordova platform rm android && ionic cordova platform add android",
        "sdk:list:images": "$ANDROID_SDK_ROOT/tools/bin/sdkmanager --list | grep -oP 'system-images/android-[0-9]+/google_apis/x86/'",
        "sdk:install": "$ANDROID_SDK_ROOT/tools/bin/sdkmanager --install \"system-images;android-$npm_config_androidApiLevel;google_apis;x86_64\"",
        "sdk:uninstall": "$ANDROID_SDK_ROOT/tools/bin/sdkmanager --uninstall \"system-images;android-$npm_config_androidApiLevel;google_apis;x86_64\"",
        "avd:list": "$ANDROID_SDK_ROOT/tools/bin/avdmanager list avd",
        "avd:list:target": "$ANDROID_SDK_ROOT/tools/bin/avdmanager list target",
        "avd:list:devices": "$ANDROID_SDK_ROOT/tools/bin/avdmanager list device",
        "avd:create": "$ANDROID_SDK_ROOT/tools/bin/avdmanager create avd --force --name $npm_config_name --abi google_apis/x86_64 --package \"system-images;android-$npm_config_androidApiLevel;google_apis;x86_64\" --device \"$npm_config_deviceName\"",
        "avd:delete": "$ANDROID_SDK_ROOT/tools/bin/avdmanager delete avd --name $npm_config_name",
        "avd:run": "$ANDROID_SDK_ROOT/tools/emulator -avd $npm_config_name",
        "sim:list": "ionic cordova run ios --list",
        "sim:run:iPhoneX": "ionic cordova run ios -lc --target=\"5095D256-6A42-4F3A-BBF2-83E7308B8EAC\"",
        "sim:run:iPhone11": "ionic cordova run ios -lc --target=\"CC7F0E96-1B28-47EF-AD47-EDEF78A7C91B\"",
        "sim:run:iPad": "ionic cordova run ios -lc --target=\"F9500178-E9F9-401A-A083-BE504325F51D\""
    },
